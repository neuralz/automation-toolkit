{
  "name": "ercdex-automation-toolkit",
  "description": "ERC dEX Automation Toolkti",
  "author": "ERC dEX <lautry@ercdex.com>",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "build:web": "webpack --mode=development",
    "compile": "rm -rf out && electron-webpack",
    "release": "yarn compile && electron-builder",
    "generate:aqueduct:remote": "yarn swagger:gen:aqueduct-remote && ts-node --project tsconfig.ts-node.json ./src/node/market-maker-api/swagger/generate-client.ts --baseApiUrl=http://localhost:8700 --namespace AqueductRemote",
    "generate:client": "yarn swagger:gen:market-maker-api && ts-node --project tsconfig.ts-node.json ./src/market-maker-ui/swagger/generate-client.ts --baseApiUrl=http://localhost:8662 --namespace Dashboard",
    "start": "electron-webpack dev",
    "test": "rm -rf ./test-data && NODE_ENV=test mocha -t 15000 -r ts-node/register src/tests/**/*.spec.ts",
    "tsoa:gen": "yarn tsoa:aqueduct-remote && yarn tsoa:market-maker-api",
    "tsoa:aqueduct-remote": "yarn swagger:gen:aqueduct-remote && yarn routes:gen:aqueduct-remote",
    "routes:gen:aqueduct-remote": "tsoa --configuration aqueduct-remote-tsoa.json routes",
    "swagger:gen:aqueduct-remote": "tsoa --configuration aqueduct-remote-tsoa.json swagger",
    "tsoa:market-maker-api": "yarn swagger:gen:market-maker-api && yarn routes:gen:market-maker-api",
    "routes:gen:market-maker-api": "tsoa --configuration=market-maker-api-tsoa.json routes",
    "swagger:gen:market-maker-api": "tsoa --configuration=market-maker-api-tsoa.json swagger"
  },
  "dependencies": {
    "0x.js": "0.36.0",
    "@0xproject/subproviders": "^0.10.2",
    "aqueduct": "^1.2.3",
    "body-parser": "^1.18.2",
    "cryptocompare": "^0.5.0",
    "ethereumjs-wallet": "^0.6.0",
    "express": "^4.16.3",
    "html5-websocket": "^2.0.2",
    "method-override": "^2.3.10",
    "moment": "^2.22.0",
    "nedb": "^1.8.0",
    "node-fetch": "^2.1.2",
    "request": "^2.85.0",
    "request-promise-native": "^1.0.5",
    "source-map-support": "^0.5.6",
    "web3": "0.20.0",
    "web3-provider-engine": "14.0.5"
  },
  "devDependencies": {
    "@0xproject/typescript-typings": "^0.2.0",
    "@types/body-parser": "^1.16.8",
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.11.1",
    "@types/handlebars": "^4.0.37",
    "@types/method-override": "^0.0.31",
    "@types/mocha": "^5.2.0",
    "@types/nedb": "^1.8.5",
    "@types/node": "^10.1.2",
    "@types/react": "^16.0.7",
    "@types/react-dom": "^16.0.2",
    "@types/react-router": "^4.0.15",
    "@types/react-router-dom": "^4.2.6",
    "@types/request-promise": "^4.1.41",
    "@types/request-promise-native": "^1.0.14",
    "@types/superagent": "^3.5.8",
    "@types/yargs": "^11.0.0",
    "asar": "^0.14.3",
    "chai": "^4.1.2",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.16",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.28.7",
    "electron": "^2.0.2",
    "electron-builder": "^20.15.1",
    "electron-updater": "^2.21.10",
    "electron-webpack": "^2.1.2",
    "electron-webpack-ts": "^2.0.2",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "handlebars": "^4.0.11",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "mocha": "^5.1.1",
    "node-sass": "^4.5.3",
    "react": "^16.3.1",
    "react-dom": "^16.0.0",
    "react-image-lightbox": "^4.6.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "superagent": "^3.8.2",
    "ts-loader": "^4.3.0",
    "ts-node": "^5.0.1",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "tslint-react": "^3.5.1",
    "tsoa": "^2.1.4",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.9.1",
    "webpack-cli": "^2.1.4",
    "webpack-dev-server": "^3.1.3",
    "webpack-livereload-plugin": "^0.11.0",
    "webpack-node-externals": "^1.7.2",
    "webpack-sources": "^1.1.0",
    "yargs": "^11.0.0"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ERCdEX/automation-toolkit"
  },
  "build": {
    "appId": "ercdex",
    "productName": "ERC dEX Automation Toolkit",
    "files": [
      "src/node/**/*",
      "src/market-maker-ui/**/*",
      "!**/*.ts",
      "!**/*.js.map",
      "!**/*.js.json",
      "!**/aqueduct/docs/**",
      "!**/aqueduct/guides/**",
      "!LICENSE"
    ],
    "directories": {
      "buildResources": "src/node/build",
      "output": "out"
    },
    "mac": {
      "category": "public.app-category.developer-tools",
      "target": ["dmg"]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32",
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "target": [
        "deb"
      ]
    },
    "publish": [
      "github"
    ]
  },
  "electronWebpack": {
    "main": {
      "sourceDirectory": "src/node",
      "webpackConfig": "webpack.main.additions.js"
    },
    "renderer": {
      "sourceDirectory": "src/market-maker-ui",
      "webpackConfig": "webpack.renderer.additions.js"
    }
  }
}
